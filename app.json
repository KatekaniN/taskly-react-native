{
  "expo": {
    "name": "taskly",
    "slug": "taskly",
    "version": "1.0.0",
    "scheme": "taskly",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "fonts": [
      {
        "asset": "@expo/vector-icons/fonts/MaterialIcons.ttf"
      },
      {
        "asset": "@expo/vector-icons/fonts/MaterialCommunityIcons.ttf"
      },
      {
        "asset": "@expo/vector-icons/fonts/Ionicons.ttf"
      },
      {
        "asset": "@expo/vector-icons/fonts/FontAwesome.ttf"
      },
      {
        "asset": "@expo/vector-icons/fonts/FontAwesome5_Regular.ttf"
      },
      {
        "asset": "@expo/vector-icons/fonts/FontAwesome5_Solid.ttf"
      },
      {
        "asset": "@expo/vector-icons/fonts/FontAwesome5_Brands.ttf"
      },
      {
        "asset": "@expo/vector-icons/fonts/AntDesign.ttf"
      },
      {
        "asset": "@expo/vector-icons/fonts/Entypo.ttf"
      },
      {
        "asset": "@expo/vector-icons/fonts/Feather.ttf"
      }
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.knyamandi99.taskly"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b0cdfe74-e770-4a1f-b703-5d5b0a68fcdb"
      }
    }
  }
}
